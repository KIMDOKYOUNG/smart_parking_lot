<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Smart_Parking_lot">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mainicon"
        android:label="주차의 민족"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Smart_Parking_lot.ParkingLotActivity"></activity>
        <activity android:name="com.example.Smart_Parking_lot.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>